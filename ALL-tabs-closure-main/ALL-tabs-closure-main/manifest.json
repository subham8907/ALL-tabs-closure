{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "author": "Subham Mahesh",
  "version": "1.10",
  "description": "__MSG_extDescription__",
  "permissions": [
    "storage",
    "browsingData"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "default_locale": "en",
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Close All Tabs",
    "default_popup": "popup/popup.html"
  },
  "options_page": "option/options.html",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "commands": {
    "closeTabs": {
      "suggested_key": {
        "default": "Ctrl+X",
        "mac": "Ctrl+X"
      },
      "description": "__MSG_shortcutCloseTabs__"
    },
    "openSettings": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "Ctrl+Q"
      },
      "description": "__MSG_shortcutOpenSettings__"
    }
  }
}
